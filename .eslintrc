{
    "ecmaFeatures": {
        "arrowFunctions": true,
        "binaryLiterals": false,
        "blockBindings": true,
        "classes": true,
        "defaultParams": true,
        "destructuring": true,
        "generators": false,
        "modules": true,
        "objectLiteralComputedProperties": true,
        "objectLiteralDuplicateProperties": false,
        "objectLiteralShorthandMethods": true,
        "objectLiteralShorthandProperties": true,
        "object": true,
        "restParams": true,
        "spread": true,
        "super": false,
        "templateStrings": true,
        "jsx": true,
        "experimentalObjectRestSpread": true
    },
    "environment": {
        "browser": true,
        "es6": true
    },
    "rules": {
        "eqeqeq": 2
    }
}